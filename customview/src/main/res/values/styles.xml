<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <declare-styleable name="IconViewClose">
        <attr name="iconSize" format="dimension"/>
        <attr name="iconStrokeWidth" format="dimension"/>
        <attr name="iconPadding" format="dimension"/>
        <attr name="lineColor" format="color"/>
        <attr name="lineColorPressed" format="color"/>
        <attr name="lineColor1" format="color"/>
        <attr name="lineColor2" format="color"/>
        <attr name="circleBg" format="boolean"/>
        <attr name="circleColor" format="color"/>
        <attr name="circleStrokeColor" format="color"/>
        <attr name="circleStrokeWidth" format="dimension"/>
        <attr name="circlePadding" format="dimension"/>
    </declare-styleable>

</resources>
